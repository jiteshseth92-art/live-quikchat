  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "server.js",
      "public/**/*.js",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "prettier"
