{
  "name": "quikchat-production",
  "version": "3.0.0",
  "description": "QuikChat - Global Random Video Chat Platform with Earning System",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "prod": "NODE_ENV=production node server.js",
    "debug": "DEBUG=* nodemon server.js",
    "test": "jest --verbose --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "jest --testPathPattern=e2e",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write \"**/*.{js,json,md,html,css}\"",
    "format:check": "prettier --check \"**/*.{js,json,md,html,css}\"",
    "build": "echo 'Building production assets...' && npm run generate-sitemap && npm run generate-robots",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "deploy": "npm run build && npm run deploy:firebase",
    "deploy:firebase": "firebase deploy --only hosting,functions",
    "deploy:heroku": "git push heroku main",
    "deploy:render": "echo 'Deploying to Render...'",
    "docker:build": "docker build -t quikchat .",
    "docker:run": "docker run -p 3000:3000 -e NODE_ENV=production quikchat",
    "docker:push": "docker tag quikchat:latest your-registry/quikchat:latest && docker push your-registry/quikchat:latest",
    "pm2:start": "pm2 start server.js --name quikchat --max-memory-restart 300M",
    "pm2:stop": "pm2 stop quikchat",
    "pm2:restart": "pm2 restart quikchat",
    "pm2:logs": "pm2 logs quikchat",
    "pm2:monit": "pm2 monit",
    "pm2:save": "pm2 save",
    "pm2:setup": "pm2 startup && pm2 save",
    "backup:db": "node scripts/backupDatabase.js",
    "restore:db": "node scripts/restoreDatabase.js",
    "backup:full": "npm run backup:db && tar -czf backup-$(date +%Y%m%d-%H%M%S).tar.gz data/ backups/ logs/",
    "seed": "node scripts/seedDatabase.js",
    "seed:test": "NODE_ENV=test node scripts/seedDatabase.js",
    "generate:sitemap": "node scripts/generateSitemap.js",
    "generate:robots": "node scripts/generateRobots.js",
    "stats": "node scripts/generateStats.js",
    "monitor": "node scripts/monitor.js",
    "health": "curl -f http://localhost:3000/health || exit 1",
    "security:scan": "npm audit",
    "security:fix": "npm audit fix",
    "deps:update": "npm-check-updates -u",
    "deps:clean": "npm prune",
    "cert:generate": "openssl req -nodes -new -x509 -keyout server.key -out server.cert -subj \"/C=US/ST=State/L=City/O=Organization/CN=localhost\"",
    "docs:generate": "jsdoc -c jsdoc.json",
    "performance:test": "autocannon -c 100 -d 30 http://localhost:3000",
    "stress:test": "artillery run artillery.yml"
  },
